#include <iostream>
#include <set>
#include <vector>

using namespace std;

class dice
{
    public:
	int top, bot;
	set<int> rest;
    dice()
    {}
    dice(int n)
    {
	top = n;
    }
};

int main()
{
    int n;
    bool no = false;
    cin>>n;
    vector<dice> dices;
    int top, n1, n2;
    cin>>top>>n1>>n2;
    dice d(top);
    d.bot = 7-top;
    dices.push_back(d);
    for(int a = 1; a < n; a++)
    {
	int h, j, ptop = dices[a-1].bot;
	dice t;
	cin>>h>>j;
	t.rest.insert(h);
	t.rest.insert(7-h);
	t.rest.insert(j);
	t.rest.insert(7-j);
	if(t.rest.find(ptop) == t.rest.end())
	{
	    t.bot = ptop;
	    dice.push_back(t);
	}
	else
	{
	    no = true;
	    break;
	}
    }
    if(no)
	cout<<"NO"<<endl;
    else
	cout<<"YES"<<endl;
    return 0;
}
