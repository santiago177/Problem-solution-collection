#include <iostream>

using namespace std;

int abs(int n)
{
   return (n < 0) ? -n: n;
}

int main()
{
    bool matrix [5][5];
    int x, y;
    for(int a = 0; a < 5; a++)
    {
	for(int b = 0; b < 5; b++)
	{
	    cin>>matrix[a][b];
	    if(matrix[a][b] == 1)
	    {
	    	x = a; 
		y = b;
	    }
	}
    }
    return 0;
}
