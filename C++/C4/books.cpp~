#include <iostream>
#include <queue>

using namespace std;

int main()
{
    long long s, t, count = 0;
    cin>>s>>t;
    priority_queue<long long> pq;
    for(long long a = 0; a < s; a++)
    {
	long long p;
	cin>>p;
	pq.push(-p);
    }
    for(long long a = 0; a < s; a++)
    {
	long long g = -pq.top();
	pq.pop();
	t -= g;
	if(t > 0)
	    count++;
	else
	    break;
    }
    cout<<count;
    return 0;
}

