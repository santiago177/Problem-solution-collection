#include <iostream>

using namespace std;

int main()
{
    short i, ans;
    bool cups[3];
    cin>>i;
    ans = i-1;
    cups[0] = cups[1] = cups[2] = false;
    cups[i-1] = true;
    for(int a = 0; a < 3; a++)
    {
	short s, t;
	cin>>s>>t;
	//cout<<cups[0]<<cups[1]<<cups[2]<<endl;
	if(cups[s-1] || cups[t-1])
	{
	    //cout<<"here"<<endl;
	    if(cups[s-1])
		ans = t-1;
	    else
		ans = s-1;
	    cups[s-1] ^= 1;
	    cups[t-1] ^= 1;
	}
    }
    cout<<ans+1;
    return 0;
}
