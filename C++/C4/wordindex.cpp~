#include <iostream>
#include <cstdio>
#include <set>
#include <map>
#include <queue>

using namespace std;

//map<string, int> word_list;
set<string> word_list;

void fill()
{
    queue<string> q;
    for(char a = 'a'; a <= 'z'; a++)
    {
	string t = "";
	t += a;
	//cout<<t<<endl;
	q.push(t);
	word_list.insert(t);
    }
    while(!q.empty())
    {
	string t = q.front();
	string copy;
	//cout<<copy<<endl;
	q.pop();
	for(char a = t[t.size()-1]+1; a <= 'z'; a++)
	{
	    copy = t;
	    copy += a;
	    if(copy.size() <= 5)
	    {
		cout<<copy<<", ";
		q.push(copy);
		word_list.insert(copy);
	    }
	}
    }
}

