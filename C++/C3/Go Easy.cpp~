#include <iostream>
#include <utility>
#include <cstdio>
#include <vector>

using namespace std;

class station
{
    public:
	vector<vector<station> > trains;
	vector<vector<station> > buses;
	int zone;
	station(int n)
	{
	    zone = n;
	}
	station()
	{
	    zone = 0;
	}
};

int main()
{
    freopen("gs.in", "r", stdin);
    int z, s;
    while(cin>>z>>s && z != 0)
    {
	vector<station> graph(s);
	for(int a = 0; a < z; a++)
	{
	    int h, p;
	    cin>>p;
	    for(int b = 0; b < p; b++)
	    {
		cin>>h;
		graph[h-1].zone = a;
	    }
	}
	for(int a = 0; a < s; a++)
	    cout<<graph[a].zone<<endl;
    }
    return 0;
}
